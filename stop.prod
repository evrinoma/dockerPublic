#!/bin/bash

docker-compose -f docker-compose.clean.yml down
sed -i -e "/php74\.clean\.public/d" /etc/hosts

for variable in `docker images | grep 'evrinoma/php74.clean.public\|evrinoma/db.maria.public' | awk '{print $3}'`
do
    if [ ! -z "$variable" ]; then
            docker rmi "$variable"
    fi
done

